 
<h1>{{title}}</h1>
<hr/>

<div class="table-responsive">
    <table class="table table-bordered table-striped table-hover" >
        <thead>
            <tr>
                <th>ID</th>
                <th>Username</th>
                <th>Email</th><th>Password</th><th>Action</th>
            </tr>
        </thead>
        <tbody>
            {{#each data}}
            <tr class="">
                <td>{{this._id}}</td>
                <td>{{this.username}}</td>
                <td>{{this.email}}</td>
                <td>{{this.password}}</td>
                <td>
                    <a class="btn btn-success me-2" href="/form/edit/{{this._id}}" ><i class="bi bi-pen"></i></a >
                    <a class="btn btn-danger me-2" href="/form/delete/{{this._id}}" ><i class="bi bi-trash"></i></a >
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>

